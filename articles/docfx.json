{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/docfx.json",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md"
        ],
        "src": ".",
        "dest": "."
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "_repo.en-us/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure/developer/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/azure-dev-docs"
    },
    "fileMetadata": {
      "feedback_product_url" : {
        "articles/python/**/*.md": "https://github.com/Azure/azure-sdk-for-python/issues",
        "**/ansible/**/*.md": "https://feedback.azure.com/forums/34192--general-feedback",
        "**/chef/**/*.md": "https://feedback.azure.com/forums/34192--general-feedback",
        "**/jenkins/**/*.md": "https://feedback.azure.com/forums/34192--general-feedback",
        "**/terraform/**/*.md": "https://feedback.azure.com/forums/34192--general-feedback"
      },
      "product_feedback_url": {
        "articles/python/tutorial-deploy-app-service-on-linux*.md" : "https://github.com/Microsoft/vscode-azureappservice/issues",
        "articles/python/tutorial-deploy-containers*.md" : "https://github.com/Microsoft/vscode-azureappservice/issues",
        "articles/python/tutorial-vs-code-serverless-python*.md" : "https://github.com/Microsoft/vscode-azurefunctions/issues"
      },
      "_op_documentIdPathDepotMapping": {
        "python": {
          "depot_name": "MSDN.python-sdk",
          "folder_relative_path_in_docset": "docs-ref-conceptual"
        }
      },
      "author": {
        "java/**/**.md": "bmitchell287",
        "go/**/*.md": "barbkess",
        "python/**/*.md": "kraigb",
        "ansible/**/*.md": "TomArcherMsft",
        "chef/**/*.md": "TomArcherMsft",
        "jenkins/**/*.md": "TomArcherMsft",
        "terraform/**/*.md": "TomArcherMsft"
      },
      "manager": {
        "**/*.md": "barbkess"
      },
      "ms.author": {
        "java/**/**.md": "brendm",
        "go/**/*.md": "barbkess",
        "python/**/*.md": "kraigb",
        "ansible/**/*.md": "tarcher",
        "chef/**/*.md": "tarcher",
        "jenkins/**/*.md": "tarcher",
        "terraform/**/*.md": "tarcher"
      },
      "ms.devlang": {
        "java/**/**.md": "java",
        "python/**/*.md": "python",
        "go/**/**.md": "go"
      },
      "ms.service": {
        "go/**/**.md": "azure",
        "java/**/**.md": "azure-java",
        "ansible/**/*.md": "ansible",
        "chef/**/*.md": "chef",
        "jenkins/**/*.md": "jenkins",
        "terraform/**/*.md": "terraform"
      },
      "titleSuffix": {
        "ansible/**/*.yml" : "Ansible",
        "chef/**/*.yml" : "Chef",
        "jenkins/**/*.yml" : "Jenkins",
        "terraform/**/*.yml" : "Terraform"
      },
      "ms.topic": {
        "**/includes/**/**.md": "include"
      },
      "ms.prod": {
        "python/**/*.md": "azure-python"
      }
    },
    "template": [],
    "dest": {
      ".": "azure-dev-docs",
      "go": "azure-go",
      "python": "azure-python"
    },
    "markdownEngineName": "markdig"
  }
}
