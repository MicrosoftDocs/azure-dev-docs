{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "**/docfx.json"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/azure-dotnet/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "Azure.azuredotnet",
          "folder_relative_path_in_docset": "docs-ref-conceptual"
        }
      }
    },
    "fileMetadata": {},
    "template": [],
    "dest": "azure-dotnet",
    "markdownEngineName": "markdig"
  }
}
